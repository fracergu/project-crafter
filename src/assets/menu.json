    "Rust": {
      "dependency": {
        "name": "Rust",
        "installationUrl": "https://www.rust-lang.org/tools/install",
        "checkCommand": "rustc --version"
      },
      "frameworks": {
        "Cargo": {
          "Default": {
            "command": "cargo new ${projectName}"
          },
          "WebAssembly": {
            "command": "cargo new ${projectName} --lib"
          }
        }
      }
    },
    "Go": {
      "dependency": {
        "name": "Go",
        "installationUrl": "https://golang.org/doc/install",
        "checkCommand": "go version"
      },
      "frameworks": {
        "Default": {
          "command": "go mod init ${projectName}"
        },
        "Gin": {
          "command": "go get -u github.com/gin-gonic/gin"
        },
        "Echo": {
          "command": "go get -u github.com/labstack/echo/v4"
        },
        "Fiber": {
          "command": "go get -u github.com/gofiber/fiber/v2"
        }
      }
