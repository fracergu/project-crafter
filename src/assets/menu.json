            "npm": {
              "dependency": {
                "name": "npm",
                "installationUrl": "https://www.npmjs.com/get-npm",
                "checkCommand": "npm --version"
              },
              "command": "npm create-react-app ${projectName}"
            },
            "yarn": {
              "dependency": {
                "name": "Yarn",
                "installationUrl": "https://classic.yarnpkg.com/en/docs/install",
                "checkCommand": "yarn --version"
              },
              "command": "yarn create react-app ${projectName}"
            },
            "pnpm": {
              "dependency": {
                "name": "pnpm",
                "installationUrl": "https://pnpm.io/installation",
                "checkCommand": "pnpm --version"
              },
              "command": "pnpm create-react-app ${projectName}"
            }
          },
