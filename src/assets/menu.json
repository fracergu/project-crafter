      "dependency": {
        "name": "Python",
        "installationUrl": "https://www.python.org/downloads/",
        "checkCommand": "python --version"
      },
      "frameworks": {
        "Django": {
          "dependency": {
            "name": "Django",
            "installationUrl": "https://www.djangoproject.com/download/",
            "checkCommand": "django-admin --version"
          },
          "command": "django-admin startproject ${projectName}"
        },
