{
  "technologies": {
    "Node": {
      "dependency": {
        "name": "Node.js",
        "installationUrl": "https://nodejs.org/en/download/",
        "checkCommand": "node --version"
      },
      "frameworks": {
        "React": {
          "Create React App": {
            "command": "npx create-react-app ${projectName}"
          },
          "Vite": {
            "JavaScript": {
              "command": "npm create vite@latest ${projectName} -- --template react"
            },
            "TypeScript": {
              "command": "npm create vite@latest ${projectName} -- --template react-ts"
            }
          }
        },
        "Angular": {
          "dependency": {
            "name": "Angular CLI",
            "installationUrl": "https://angular.io/cli",
            "checkCommand": "ng --version"
          },
          "command": "ng new ${projectName}"
        },
        "Express": { "command": "npx express-generator ${projectName}" }
      }
    }
  }
}
